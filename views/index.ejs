}


.about .about_main .about_image .about_small_image img:hover{
    opacity: 1;
}

.about .about_main .about_image h3{
    font: 28px, sans-serif;
    color: #011E42;

}

.about .about_main .about_image p{
    font: 28px, sans-serif;
    color:#c72020 ;
    
}

.about .about_main .image_contaner{
    padding: 10px;
