/*Grounds*/

.ground{
    width: 100%;
    height: 120vh;
    padding: 25px 0;
}

.ground h1{
    margin: 35px 0;
    font-size: 60px;
    display: flex;
    align-items: center;
    justify-content: center;
    text-transform: uppercase;
    background: linear-gradient(to right, #c72020,#6c14d0);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;    
}

.ground .box{
    width: 90%;
    margin: 0 auto;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    grid-gap: 25px 0;
    gap: 3rem;
}

.ground .box .card{
    width: 400px;
    height: 600px;
    box-shadow: 0 0 10px #6c14d0;
    border-radius: 5px;
    text-align: center;
    padding: 10px 20px;
    background: #f6f6f6;
    justify-content: center;
}

.ground .box .card .image{
    display: flex;
    align-items: center;
    justify-content: center;
}

.ground .box .card .image img{
    width: 300px;
    height: 250px;
    margin: 15px 0;
    transition: 0.3s;
}

.ground .box .card:hover .image img{
    transform: scale(1.1);
}

.ground .box .card .small_card i{
    width: 45px;
    height: 45px;
    border-radius: 5px;
    font-size: 18px;
    margin: 2px 0;
    line-height: 40px;
    border: 2px solid #999999;
    transition: 0.2s;
}

.ground .box .card .small_card i:hover{
    color: #c72020;
}

.ground .box .card .grounds_text h2{
    font-size: 20px;
    margin-top: 60px;
}

.ground .box .card .grounds_text p{
    color: #91919191;
    line-height: 21px;
    margin: 8px 0;
}

.ground .box .card .grounds_text h3{
    margin: 7px 0;
}

.ground .box .card .grounds_text .grounds_star{
    color: orange;
    margin-bottom: 19px;
    cursor: pointer;
}

.ground .box .card .grounds_text .btn{
    text-decoration: none;
    padding: 10px 20px;
    background: linear-gradient(to right, #c72020 , #6c14d0);
    color: white;
}


/*About*/

.about{
    width: 100%;
    height: 100vh;
    padding: 35px 0;
}

.about h1{
    font-size: 60px;
    margin-top: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: black;
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    text-transform: uppercase;
}

.about h1 span{
    background: linear-gradient(to right, #c72020 , #6c14d0);
    -webkit-background-clip: text;
